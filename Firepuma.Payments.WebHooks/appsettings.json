{
  "MongoDb": {
    "ConnectionString": "",
    "DatabaseName": "FirepumaPayments",
    "AuthorizationFailuresCollectionName": "AuthorizationFailures",
    "CommandExecutionsCollectionName": "CommandExecutions",
    "AppConfigurationsCollectionName": "ApplicationConfigs",
    "PaymentsCollectionName": "Payments",
    "NotificationTracesCollectionName": "NotificationTraces"
  },

  "IntegrationEvents": {
    "FirepumaPaymentsWorkerProjectId": "firepuma-microservices",
    "FirepumaPaymentsWorkerTopicId": "payments-service",
    "NotificationsServiceProjectId": "firepuma-microservices",
    "NotificationsServiceTopicId": "notifications-service"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Firepuma": "Debug"
    },
    "GoogleLogging": {
      "IsEnabled": true,
      "BufferType": "Timed",
      "TimeBufferWaitTimeMilliseconds": "3000",
      "RetryType": "Retry",
      "RetryExceptionHandling": "Propagate"
    }
  },
  "AllowedHosts": "*"
}
